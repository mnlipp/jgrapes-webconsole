# Used for JavaExec tasks that run bnd (cli)
-plugin.RunRepo: \
    aQute.bnd.deployer.repository.LocalIndexedRepo; \
        name = RunRepo; \
        local = ${workspace}/build/runRepo

-diffignore: Git-Descriptor, Git-SHA

-sources: true

# JUnit
junit: org.apache.servicemix.bundles.junit; version=4.12

-fixupmessages.general: \
    "warning: osgi.serviceloader capability found with no *"    

