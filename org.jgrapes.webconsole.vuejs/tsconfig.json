{
  "compilerOptions": {
    "target": "es2020",
    "module": "ES2020",
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "importHelpers": true,
    "strict": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "lib": ["DOM", "ES2020"],
    "baseUrl": "..",
    "paths": {
      "@JGConsole": ["org.jgrapes.webconsole.base/build/generated/resources/org/jgrapes/webconsole/base/JGConsole"],
      "@GridStack": ["org.jgrapes.webconsole.provider.gridstack/node_modules/gridstack/dist/gridstack"],
      "@Vue": ["org.jgrapes.webconsole.provider.vue/node_modules/vue/dist/vue"],
      "@Aash": ["aash-vue-components/lib/AashPlugin"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
